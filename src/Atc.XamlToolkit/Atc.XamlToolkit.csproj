<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Title>Atc.XamlToolkit</Title>
    <PackageId>Atc.XamlToolkit</PackageId>
    <PackageTags>mvvm,relaycommand</PackageTags>
    <Description>A base library for building XAML applications with the MVVM design pattern.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Atc" Version="2.0.552" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\Atc.XamlToolkit.SourceGenerators\bin\$(Configuration)\netstandard2.0\Atc.XamlToolkit.SourceGenerators.dll" Pack="true" PackagePath="analyzers/dotnet/cs" Visible="false" />
    <None Remove="_._" />
  </ItemGroup>

</Project>